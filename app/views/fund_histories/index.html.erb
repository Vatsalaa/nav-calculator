<p id="notice"><%= notice %></p>

<h1>Fund Histories</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Nav</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fund_histories.each do |fund_history| %>
      <tr>
        <td><%= fund_history.date %></td>
        <td><%= fund_history.nav %></td>
        <td><%= link_to 'Show', fund_history %></td>
        <td><%= link_to 'Edit', edit_fund_history_path(fund_history) %></td>
        <td><%= link_to 'Destroy', fund_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fund History', new_fund_history_path %>
